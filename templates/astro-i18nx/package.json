{
  "name": "astro-i18nx",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst bind astro dev",
    "build": "astro build",
    "build:bind": "sst bind astro build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "lint": "eslint .",
    "fmt": "prettier . --write",
    "i18n:extract": "astro-i18n extract",
