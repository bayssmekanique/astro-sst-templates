{
  "name": "astro-templates",
  "private": true,
  "type": "module",
  "workspaces": [
    "templates/*"
  ],
  "scripts": {
    "lint": "turbo run lint",
    "fmt": "turbo run fmt",
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "dev:ssr": "dotenv -- turbo run dev --filter=astro-ssr",
    "dev:hybrid": "dotenv -- turbo run dev --filter=astro-hybrid",
    "dev:streamed": "dotenv -- turbo run dev --filter=astro-streamed",
    "dev:static": "dotenv -- turbo run dev --filter=astro-static",
    "dev:edge": "dotenv -- turbo run dev --filter=astro-edge",
    "build:ssr": "dotenv -- turbo run build --filter=astro-ssr",
    "build:hybrid": "dotenv -- turbo run build --filter=astro-hybrid",
    "build:streamed": "dotenv -- turbo run build --filter=astro-streamed",
    "build:static": "dotenv -- turbo run build --filter=astro-static",
    "build:edge": "dotenv -- turbo run build --filter=astro-edge",
    "deploy": "dotenv -- turbo run deploy --concurrency=1 --continue",
    "deploy:ssr": "dotenv -- turbo run deploy --filter=astro-ssr",
    "deploy:hybrid": "dotenv -- turbo run deploy --filter=astro-hybrid",
    "deploy:streamed": "dotenv -- turbo run deploy --filter=astro-streamed",
    "deploy:static": "dotenv -- turbo run deploy --filter=astro-static",
    "deploy:edge": "dotenv -- turbo run deploy --filter=astro-edge",
    "remove": "dotenv -- turbo run remove --concurrency=1 --continue",
    "remove:ssr": "dotenv -- turbo run remove --filter=astro-ssr",
    "remove:hybrid": "dotenv -- turbo run remove --filter=astro-hybrid",
    "remove:streamed": "dotenv -- turbo run remove --filter=astro-streamed",
    "remove:static": "dotenv -- turbo run remove --filter=astro-static",
    "remove:edge": "dotenv -- turbo run remove --filter=astro-edge"
  },
  "dependencies": {
    "@astrojs/mdx": "^1.1.2",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.2",
    "astro": "^3.3.3",
    "astro-robots-txt": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.9.0",
    "astro-sst": "^2.32.2",
    "aws-cdk-lib": "2.101.1",
    "constructs": "10.2.69",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.52.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "fs-extra": "^11.1.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-sort-imports": "^1.7.2",
    "sst": "^2.32.2",
    "turbo": "latest"
  },
  "packageManager": "npm@9.8.1"
}
